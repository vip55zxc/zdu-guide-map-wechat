.page{
  width: 100vw;
  height: 100vh;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.box {
  width: 76%;
  font-size: 28rpx;
  color: #333;
}

.feedback-title {
  font-size: 40rpx;
}

.allnr {
  color: #888;
  margin-top: 30rpx;
  margin-bottom: 50rpx;
}

.nr {
  line-height: 2;
}

.btn-title {
  font-size: 30rpx;
  font-weight: bolder;
  margin-bottom: 15rpx;
}

.fg {
  background: #e0e3da;
  width: 100%;
  height: 1rpx;
  margin-bottom: 90rpx;
}

.banner-image{
  width: 100%
}

.p{
  background-image: linear-gradient(90deg,#3e60ce 0,#5983fc 51%,#3e60ce);
  /* background-image: linear-gradient(90deg,#ff67cb 0,#ffcc67 51%,#ff67cb); */
  -webkit-background-clip: text;
  color: transparent;
}
